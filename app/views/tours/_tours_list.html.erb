<% tours.each do |tour| %>
  <div class="col-md-4">
      <div class="panel panel-default">
          <div class="panel-heading preview">
              <%= image_tag tour.cover_photo(:medium) %>
          </div>
          <div class="panel-body">
              <%= link_to tour.title, tour%>
          </div>
      </div>
  </div>
<% end %>